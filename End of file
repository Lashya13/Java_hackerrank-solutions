import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner Scan = new Scanner(System.in);
    int i = 1;
    while(Scan.hasNext()) {
        System.out.println(i +" "+ Scan.nextLine());
        i++;
    }
    Scan.close();
        
        
       
    }
}
